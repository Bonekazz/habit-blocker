{
	"manifest_version": 2,
	"name": "Habit Blocker",
	"description": "Blocks your bad habit of openning bad websites for you",
	"version": "1.0",
	"permissions": [
        "storage",
		"tabs",
		"*://*/*"
	],
    "background": {
        "scripts": ["blocker.js"],
		"type": "module",
		"persistent": false
    },
	"browser_action": {
		"default_popup": "index.html"
	}
}
